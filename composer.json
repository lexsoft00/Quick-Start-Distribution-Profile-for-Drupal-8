    "drupal/dropzonejs": "~1.0",
    "drupal/ds": "~3.0",
    "drupal/embed": "~1.0",
    "drupal/entity": "~1.0",
    "drupal/entity_browser": "~1.0",
    "drupal/entity_browser_enhanced": "~1.0",
    "drupal/entity_clone": "~1.0",
    "drupal/entity_embed": "~1.0",
    "drupal/entity_reference_revisions": "~1.0",
    "drupal/entityqueue": "~1.0",
    "drupal/entityqueue_form_widget": "~1.0",
    "drupal/extlink": "~1.0",
    "drupal/features": "~3.0",
    "drupal/field_group": "~3.0",
    "drupal/focal_point": "~1.0",
    "drupal/google_analytics": "~2.0",
    "drupal/imageapi_optimize": "~2.0",
    "drupal/inline_entity_form": "~1.0",
    "drupal/inline_responsive_images": "~2.0",
    "drupal/libraries_ui": "~1.0",
