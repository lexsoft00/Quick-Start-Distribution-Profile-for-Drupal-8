{
  "name": "drupalcoders/quick_start",
  "description": "Quick Start for Drupal 8",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "lexsoft",
      "homepage": "http://drupalcoders.org",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/quick_start",
    "source": "http://cgit.drupalcode.org/quick_start"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
      },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "~1.0",
    "oomphinc/composer-installers-extender": "~1.0",
    "cweagans/composer-patches": "~1.0",
    "drupal-composer/drupal-scaffold": "~2.0",
    "drupal/drupal-library-installer-plugin": "^0.3",
    "composer-plugin-api": "^1.1",
    "drupal/core": "~8.6",
    "drupal/config_update": "~1.0",
    "drupal/config_filter": "~1.0",
    "drupal/config_ignore": "~2.0",
    "drupal/ctools": "~3.0",
    "drupal/token": "~1.0",
    "drupal/bootstrap_layouts": "~5.0",
    "drupal/layout_builder_restrictions": "~1.0",
    "drupal/ds": "~3.0",
    "drupal/views_bootstrap": "~3.0",
    "drupal/field_group": "~3.0",
    "drupal/smart_trim": "~1.0",
    "drupal/advanced_text_formatter": "~1.0",
    "drupal/block_class": "~1.0",
    "drupal/menu_block": "~1.0",
    "drupal/features": "~3.0",
    "drupal/entity": "~1.0",
    "drupal/entity_clone": "~1.0",
    "drupal/entityqueue": "~1.0",
    "drupal/entityqueue_form_widget": "~1.0",
    "drupal/inline_entity_form": "~1.0",
    "drupal/ultimate_cron": "~2.0",
    "drupal/persistent_login": "~1.0",
    "drupal/userprotect": "~1.0",
    "drupal/betterlogin": "~1.0",
    "drupal/config_perms": "~2.0",
    "drupal/roleassign": "~1.0",
    "drupal/admin_toolbar": "~1.0",
    "drupal/adminimal_admin_toolbar": "~1.0",
    "drupal/coffee": "~1.0",
    "drupal/views_bulk_operations": "~2.0",
    "drupal/views_bulk_edit": "~2.0",
    "drupal/revision_log_default": "~1.0",
    "drupal/webform": "~5.0",
    "drupal/webform_analysis": "~1.0",
    "drupal/webform_views": "~5.0",
    "drupal/default_content": "1.0-alpha7",
    "drupal/content_lock": "~1.0",
    "drupal/rabbit_hole": "~1.0",
    "drupal/yoast_seo": "~1.0",
    "drupal/fast_404": "~1.0",
    "mkalkbrenner/php-htmldiff-advanced": "~0.0.8",
    "drupal/diff": "~1.0",
    "drupal/masquerade": "~2.0",
    "drupal/menu_admin_per_menu": "~1.0",
    "drupal/drd_agent": "~3.0",
    "drupal/libraries_ui": "~1.0",
    "drupal/devel": "~2.0",
    "drupal/username_enumeration_prevention": "~1.0",
    "drupal/password_policy": "~3.0",
    "drupal/seckit": "~1.0",
    "drupal/security_review": "1.x-dev#35ebae445bb260e961e47c4c58efe7c50c228999",
    "drupal/honeypot": "~1.0",
    "drupal/shield": "~1.0",
    "drupal/captcha": "~1.0",
    "drupal/recaptcha": "~2.0",
    "drupal/mailsystem": "~4.0",
    "drupal/smtp": "~1.0",
    "drupal/maxlength": "~1.0",
    "drupal/taxonomy_access_fix": "~2.0",
    "drupal/better_exposed_filters": "~3.0",
    "drupal/autocomplete_deluxe": "~1.0",
    "drupal/extlink": "~1.0",
    "drupal/link_attributes": "~1.0",
    "drupal/entity_reference_revisions": "~1.0",
    "drupal/viewsreference": "~2.0",
    "drupal/libraries": "~3.0",
    "drupal/vmi": "~2.0",
    "drupal/varbase_media": "~6.0",
    "drupal/varbase_bootstrap_paragraphs": "~6.0",
    "drupal/varbase_landing": "~6.0",
    "drupal/varbase_search": "~6.0",
    "drupal/varbase_seo": "~6.0",
    "drupal/varbase_heroslider_media": "~6.0",
    "drupal/ckeditor_accordion": "~1.0",
    "drupal/ckeditor_bootstrap_grid": "~1.0",
    "drupal/pathologic": "~1.0",
    "drupal/ace_editor": "~1.0",
    "drupal/linkit": "~4.0",
    "drupal/ckeditor_media_embed": "~1.0",
    "drupal/anchor_link": "~1.0",
    "drupal/image_resize_filter": "1.x-dev#c3f4b23b02005859092aaff746b9f21b794adc58",
    "drupal/taxonomy_menu": "~3.0",
    "drupal/adminimal_theme": "~1.0",
    "drupal/drupalcoders_bootstrap": "~1.0",
    "npm-asset/ace-builds": "~1.0"
  },
  "require-dev": {
    "drush/drush": "~9",
    "drupal/console": "~1.0",
    "drupal/coder": "~8",
    "drupal/drupal-extension": "~3",
    "phing/phing": "~2",
    "behat/behat": "~3",
    "behat/mink": "~1",
    "behat/mink-extension": "~2.",
    "behat/mink-goutte-driver": "~1",
    "behat/mink-zombie-driver": "~1",
    "behat/mink-selenium2-driver": "~1",
    "behatch/contexts": "~3",
    "emuse/behat-html-formatter": "^0.1.0"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false,
    "preferred-install": {
      "drupal/core": "dist"
    }
  },
  "autoload": {
    "psr-4": {
      "QuickStart\\composer\\": "src/composer"
    }
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "pre-install-cmd": ["QuickStart\\composer\\ScriptHandler::checkComposerVersion"],
    "pre-update-cmd": ["QuickStart\\composer\\ScriptHandler::checkComposerVersion"],
    "post-install-cmd": [
      "QuickStart\\composer\\ScriptHandler::createRequiredFiles",
      "QuickStart\\composer\\ScriptHandler::removeGitDirectories",
      "@composer drupal-scaffold"
    ],
    "post-update-cmd": [
      "QuickStart\\composer\\ScriptHandler::createRequiredFiles",
      "QuickStart\\composer\\ScriptHandler::removeGitDirectories"
    ],
    "post-drupal-scaffold-cmd": ["QuickStart\\composer\\ScriptHandler::postDrupalScaffoldProcedure"]
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-1.x": "8.5.x-dev"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "docroot/core": ["drupal/core"],
      "docroot/profiles/{$name}": ["type:drupal-profile"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
      "docroot/libraries/slick": ["npm-asset/slick-carousel"],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
      "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "drupal-libraries": {
      "library-directory": "docroot/libraries",
      "libraries": [
          {"name": "dropzone","package": "npm-asset/dropzone"},
          {"name": "blazy", "package": "npm-asset/blazy"},
          {"name": "slick", "package": "npm-asset/slick-carousel"},
          {"name": "ace", "package": "npm-asset/ace-builds"}
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/core": {
        "Issue #2869592: Disabled update module shouldn't produce a status report warning":
        "https://www.drupal.org/files/issues/2869592-remove-update-warning-7.patch",
        "Issue #2885441: EntityReferenceAutocompleteWidget should define its size setting as an integer":
        "https://www.drupal.org/files/issues/2885441-2.patch",
        "Issue #1356276: Allow profiles to define a base/parent profile and load them in the correct order":
        "https://www.drupal.org/files/issues/2018-10-23/1356276-549-8.6.x.patch",
        "Issue #2914389: Allow profiles to exclude dependencies of their parent":
        "https://www.drupal.org/files/issues/2018-11-07/2914389-11-do-not-test.patch",
        "Issue #2720101 Label (Title) not set for Views block (exposed filters in Block)":
        "https://www.drupal.org/files/issues/2720101-53.patch",
        "Issue #2599228: Programmatically created translatable content type returns SQL error on content creation":
        "https://www.drupal.org/files/issues/2018-10-06/2599228-74.patch",
        "Issue #2893892: Fixed Warning Invalid argument supplied for foreach() in Drupal":
        "https://www.drupal.org/files/issues/patch_empty_fix.patch"
      },
      "drupal/coffee": {
        "Issue #2815381: Coffee breaks whole page when using right to left (rtl) languages in drupal 8":
        "https://www.drupal.org/files/issues/coffee-coffee_breaks_page_with_rtl-2815381-6.patch",
        "Issue #2872093: Coffee css fixes in RTL":
        "https://www.drupal.org/files/issues/2872093-10.patch"
      },
      "drupal/default_content": {
        "Issue #2900089: Invalid translation language error when selected installation language does not match demo content language":
        "https://www.drupal.org/files/issues/2900089-14.patch",
        "Issue #2886350: Allow export of all site content.":
        "https://www.drupal.org/files/issues/default_content-export_all_content-2886350-2-D8.patch"
      },
      "drupal/persistent_login": {
        "Issue #2899530: Hide user^s local task":
        "https://www.drupal.org/files/issues/persistent_login-user_task_permission-2899530-6.patch"
      },
      "drupal/ds": {
        "Issue #2920868: Fixed fatal errors when we have missing layouts, on an update." :
        "https://www.drupal.org/files/issues/error-when-layout-is-removed-2920868-4.patch",
        "Issue #2966959: Value Conflict with Layout Builder":
        "https://www.drupal.org/files/issues/2018-04-27/2966959-ds-namespace_form_for_layout_builder-4.patch"
      },
      "drupal/rabbit_hole": {
        "Issue #2961738: Ensure getOperation method exists on form":
        "https://www.drupal.org/files/issues/2018-05-10/rabbit_hole-instance_of_check-2961738-5.patch"
      },
      "drupal/login_destination": {
        "Issue #2947616: Fixed SetOptions on null causing error.":
        "https://www.drupal.org/files/issues/2018-03-09/toolbar_alter_error-2947616-11.patch"
      },
      "drupal/field_group": {
        "Issue #2921228: Accordion does not work":
        "https://www.drupal.org/files/issues/field_group_accordion-2921228-3.patch"
      },
      "drupal/ckeditor_media_embed": {
        "Issue #2900313: Add ability to embed tweets and other rich content in WYSIWYG":
        "https://www.drupal.org/files/issues/embed_rich_content_in_WYSIWYG-2900313-2.patch"
      }
    }
  }
}
