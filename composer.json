    "drupal/blazy": "~1.0",
    "drupal/block_class": "~1.0",
    "drupal/bootstrap_layouts": "~5.0",
    "drupal/captcha": "~1.0",
    "drupal/colorbox": "~1.0",
    "drupal/colorbox_inline": "~1.0",
    "drupal/ckeditor_accordion": "~1.0",
    "drupal/ckeditor_bootstrap_grid": "~1.0",
    "drupal/coffee": "~1.0",
    "drupal/config_update": "~1.0",
    "drupal/config_filter": "~1.0",
    "drupal/config_ignore": "~2.0",
    "drupal/contact_formatter": "~1.0",
    "drupal/content_lock": "~1.0",
