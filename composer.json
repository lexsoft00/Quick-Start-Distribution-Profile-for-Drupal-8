    "drupal/page_manager": "~4.0",
    "drupal/panelizer": "~4.0",
    "drupal/panels": "~4.0",
    "drupal/paragraphs": "~1.0",
    "drupal/paragraphs_previewer": "~1.0",
    "drupal/pathauto": "~1.0",
    "drupal/pathologic": "~1.0",
    "drupal/quick_v_media": "~1.0",
    "drupal/quick_v_bootstrap_paragraphs": "~1.0",
    "drupal/rabbit_hole": "~1.0",
    "drupal/recaptcha": "~2.0",
    "drupal/redirect": "~1.0",
    "drupal/schema_metatag": "~1.0",
    "drupal/search_api": "~1.0",
    "drupal/simple_sitemap": "~2.0",
    "drupal/slick": "~1.0",
    "drupal/slick_views": "~1.0",
    "drupal/slick_media": "~1.0",
    "drupal/smart_trim": "~1.0",
    "drupal/smtp": "~1.0",
